@model TrashCollector.Models.Customers

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutEmployees.cshtml";
}

<h1>Details</h1>
<!DOCTYPE html>
<style>
    #map {
        height: 400px;
        width: 400px;
    }
</style>
<br />
<br />
<div id="map"></div>

<script>
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 23.778074, lng: 90.397514 },
            zoom: 16
        });
    }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDq2D3RijkKHMwGijd4Ki_nQ7CRvkykO64&callback=initMap" async defer></script>
<div>
    <h4>Employees</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Address)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.City)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.City)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ZipCode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ZipCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ConfirmPickUp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ConfirmPickUp)
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
